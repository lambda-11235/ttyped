
listT = (\a : *. (@r : *. (@f : (@x : a. (@y : r. r)). (@b : r. r))))

nil = (\a : *. ((id (listT a))
  (\r : *. (\f : (@x : a. (@y : r. r)). (\b : r. b)))))

cons = (\a : *. (\x : a. (\xs : (listT a). ((id (listT a))
        (\r : *. (\f : (@x : a. (@y : r. r)). (\b : r.
         ((f x) (((xs r) f) b)))))))))


map = (\a : *. (\b : *. (\f : (@x : a. b). (\xs : (listT a). ((id (listT b))
  (((xs (listT b))
    (\x : a. (\y : (listT b). (((cons b) (f x)) y))))
    (nil b)))))))
